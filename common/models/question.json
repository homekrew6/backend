{
  "name": "Question",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "name": {
      "type": "string",
      "required": true
    },
    "type": {
      "type": "number",
      "required": true
    },
    "parent_id": {
      "type": "number",
      "required": false
    },
    "option": {
      "type": [
        "object"
      ]
    },
    "icon": {
      "type": "string",
      "required": false
    },
    "color": {
      "type": "string",
      "required": false
    },
    "image": {
      "type": "string",
      "required": false
    },
    "is_active": {
      "type": "boolean"
    },
    "range_name": {
      "type": "string",
      "required": false
    },
    "start_range": {
      "type": "number",
      "required": false
    },
    "end_range": {
      "type": "number",
      "required": false
    },
    "IncrementId": {
      "type": "number",
      "required": false
    },
    "selectedRadio": {
      "type": "string",
      "required": false
    },
    "rangeValue": {
      "type": "string",
      "required": false
    },
    "sliderValues": {
      "type": "string",
      "required": false
    },
    "Status": {
      "type": "boolean",
      "required": false
    },
    "isSlided": {
      "type": "string",
      "required": false,
      "default": "false"
    }
  },
  "validations": [],
  "relations": {
    "service": {
      "type": "belongsTo",
      "model": "service",
      "foreignKey": ""
    },
    "questions": {
      "type": "hasMany",
      "model": "Question",
      "foreignKey": "parent_id"
    },
    "answers": {
      "type": "hasMany",
      "model": "Answer",
      "foreignKey": ""
    },
    "serviceQuestionAnswers": {
      "type": "hasMany",
      "model": "ServiceQuestionAnswer",
      "foreignKey": ""
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
